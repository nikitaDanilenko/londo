module Math.Constants exposing (..)

import BigInt exposing (BigInt)


zeroBigInt : BigInt
zeroBigInt =
    0 |> BigInt.fromInt


oneBigInt : BigInt
oneBigInt =
    1 |> BigInt.fromInt


tenBigInt : BigInt
tenBigInt =
    10 |> BigInt.fromInt


oneHundredBigInt : BigInt
oneHundredBigInt =
    100 |> BigInt.fromInt
